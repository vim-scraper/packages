function! KeywordTrigger()
	normal mhb
	let b:id = synIDattr( synID( line("."), col(".") , 1) , "name") 
	if ( b:id == "vfpCmd" || b:id == "vfpFunc" ||  b:id == "vfpPreProc" ||  b:id == "vfpConst" ||  b:id == "vfpOperator" )
		normal gUw`h
	elseif b:id == "vfpSpecial"
		let b:k_save = &iskeyword
		let &iskeyword = "@,48-57,_,128-167,224-235"
		normal gUl`h
		let &iskeyword = b:k_save
	"elseif b:id == "vfpAbbrev"
	else
		normal `h
	endif
	unlet b:id
endfunction 

