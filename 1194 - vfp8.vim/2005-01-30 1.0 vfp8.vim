function! KeywordBuffer() 
	
	let b:k_save = &iskeyword
	let &iskeyword = "48-57,_,128-167,224-235,."
	split $VIM/vimfiles/syntax/vfp8_keywords.txt
	hide
	
	normal mhG
	let b:flags = "w"
	while search("\\h\\w*",b:flags) > 0 
		let b:flags = "W"
		
	endwhile 
	normal `h	
	let &iskeyword = &b:k_save
	
endfunction 

